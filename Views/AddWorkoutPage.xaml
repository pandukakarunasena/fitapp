<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FitApp.Views.AddWorkoutPage"
             Title="AddWorkoutPage">

    <ListView x:Name="WorkoutListView"
              ItemSelected="OnWorkoutSelected">
        <ListView.ItemTemplate>
            <DataTemplate>
                <ViewCell>
                    <Button x:Name="btnSelectWorkout"  Text="{Binding Name}" Clicked="btnSelectWorkout_Clicked"></Button>
                </ViewCell>
            </DataTemplate>
        </ListView.ItemTemplate>
    </ListView>
</ContentPage>
    
    